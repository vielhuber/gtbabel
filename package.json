{
    "scripts": {
        "js:browserify:detectdomchanges": "browserify ./components/detectdomchanges/script.js --outfile ./components/detectdomchanges/build/bundle.js --debug -g [ browserify-css --minify=true --stripComments=true ] --transform babelify",
        "js:minify:detectdomchanges": "terser --compress --mangle --output ./components/detectdomchanges/build/bundle.js ./components/detectdomchanges/build/bundle.js",
        "js:watch:detectdomchanges": "onchange ./components/*/*.js --initial --poll 50 --delay 0 --await-write-finish 50 -- onerror \"npm run js:browserify:detectdomchanges\" --title \"js:watch:detectdomchanges\" --message \"build failed\" --sound mute",
        "js:browserify:frontendeditor": "browserify ./components/frontendeditor/script.js --outfile ./components/frontendeditor/build/bundle.js --debug -g [ browserify-css --minify=true --stripComments=true ] --transform babelify",
        "js:minify:frontendeditor": "terser --compress --mangle --output ./components/frontendeditor/build/bundle.js ./components/frontendeditor/build/bundle.js",
        "js:watch:frontendeditor": "onchange ./components/*/*.js --initial --poll 50 --delay 0 --await-write-finish 50 -- onerror \"npm run js:browserify:frontendeditor\" --title \"js:watch:frontendeditor\" --message \"build failed\" --sound mute",
        "js:browserify:backend": "browserify ./components/wpbackend/script.js --outfile ./wordpress/assets/build/bundle.js --debug -g [ browserify-css --minify=true --stripComments=true ] --transform babelify",
        "js:minify:backend": "terser --compress --mangle --output ./wordpress/assets/build/bundle.js ./wordpress/assets/build/bundle.js",
        "js:watch:backend": "onchange ./components/*/*.js --initial --poll 50 --delay 0 --await-write-finish 50 -- onerror \"npm run js:browserify:backend\" --title \"js:watch:backend\" --message \"build failed\" --sound mute",
        "js": "npm-run-all --sequential js:browserify:* js:minify:*",
        "js:dev": "npm-run-all --sequential js:browserify:*",
        "css:sass:frontendeditor": "node-sass --output-style compressed ./components/frontendeditor/style.scss ./components/frontendeditor/build/bundle.css",
        "css:sass:dev:frontendeditor": "node-sass --source-map true ./components/frontendeditor/style.scss ./components/frontendeditor/build/bundle.css",
        "css:postcss:autoprefixer:frontendeditor": "postcss --use autoprefixer --output ./components/frontendeditor/build/bundle.css ./components/frontendeditor/build/bundle.css",
        "css:watch:frontendeditor": "onchange ./components/*/*.scss --initial --poll 50 --delay 0 --await-write-finish 50 -- onerror \"npm run css:sass:dev:frontendeditor\" --title \"css:watch:frontendeditor\" --message \"build failed\" --sound mute",
        "css:sass:backend": "node-sass --output-style compressed ./components/wpbackend/style.scss ./wordpress/assets/build/bundle.css",
        "css:sass:dev:backend": "node-sass --source-map true ./components/wpbackend/style.scss ./wordpress/assets/build/bundle.css",
        "css:postcss:autoprefixer:backend": "postcss --use autoprefixer --output ./wordpress/assets/build/bundle.css ./wordpress/assets/build/bundle.css",
        "css:watch:backend": "onchange ./components/*/*.scss --initial --poll 50 --delay 0 --await-write-finish 50 -- onerror \"npm run css:sass:dev:backend\" --title \"css:watch:backend\" --message \"build failed\" --sound mute",
        "css": "npm-run-all --sequential css:sass:* css:postcss:autoprefixer:*",
        "css:dev": "npm-run-all --sequential css:sass:dev:*",
        "prod": "npm-run-all --parallel js css",
        "dev": "npm-run-all --parallel js:watch:* css:watch:*"
    },
