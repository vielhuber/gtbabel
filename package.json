{
    "scripts": {
        "js:browserify": "browserify ./js/frontend/script.js --outfile ./js/frontend/build/bundle.js --debug -g [ browserify-css --minify=true ] --transform babelify",
        "js:minify": "terser --compress --mangle --output ./js/frontend/build/bundle.js ./js/frontend/build/bundle.js",
        "js": "npm-run-all --sequential js:browserify js:minify",
        "js:dev": "npm-run-all --sequential js:browserify",
        "watch:js": "onchange ./js/frontend/*.js --initial --poll 50 --delay 0 --await-write-finish 50 -- onerror \"npm run js:dev\" --title \"watch:js\" --message \"build failed\" --sound mute",
        "prod": "npm-run-all --parallel js",
        "dev": "npm-run-all --parallel watch:*"
    },
